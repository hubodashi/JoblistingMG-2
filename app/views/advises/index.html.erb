<div class="pull-right">
  <%= link_to("Add an advise", new_advise_path, :class => "btn btn-default" ) %>
</div>
<table class="table table-bordered">
  <% @advises.each do |advise| %>
  <tr>
    <td>
      <%= link_to(advise.name, advise_path(advise)) %>
    </td>
    <td>
      <%= advise.created_at %>
    </td>
  </tr>
  <% end %>
</table>
<div class="text-center">
 <%= will_paginate @advises %>
</div>
