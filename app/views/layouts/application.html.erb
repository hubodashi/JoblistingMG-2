    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="shortcut icon" href="http://localhost:3000/images/favicon.ico">
  </head>

  <body>
    <div class="container-fluid">
     <div class="row">
      <%= render "common/navbar" %>
     </div>
    <%= render "common/flashes" %>
